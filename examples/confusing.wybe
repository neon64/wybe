
def baz(foo: int, bar: int, ?result1: int, ?result2: wybe.list(int)) {
	if { foo = 3 ::
    	?result1 = 5
		?result2 = [7]
	| else ::
        ?result1 = 10
		if { bar = 9 ::
        	?result2 = [27]
        | else ::
            baz(foo + 1, bar - 1, ?ignored, ?r2)
            ?result2 = [5 | r2]
        }
    }
}

baz(2,7,?x1, ?x2)
!println(x1)
if { ?x3 = x2[1] ::
    !println(x3)
}