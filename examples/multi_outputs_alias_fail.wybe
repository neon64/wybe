use coordinate

def {noinline} bar(x: int, y: int, ?z: int, ?w: int) {
    ?z = x + y
    ?w = x - y
}

def foo(x: int, y: int, ?output: coordinate) {
    bar(x,y,?z,?w)
    # here we attempt to use the parameter `z` twice... this should fail our analysis
    ?output = coordinate(z,z)
}

?output = foo(5,9)
!println(output^x)
