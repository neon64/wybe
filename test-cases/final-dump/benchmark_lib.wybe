use benchmark

# very inefficient way of calculating fibonacci sequence
def fib(x: int) = if {
    x = 0 ::
        0
    | else :: if {
        x = 1 ::
            1
        | else ::
            fib(x-1) + fib(x-2)
    }
}

!benchmark.cpu_time({impure} { ?foo = fib(42) } , ?time)
!print(time)

!benchmark.benchmark({impure} { ?foo = fib(42) })